{
  "name": "@slate-yjs/react",
  "sideEffects": false,
  "version": "1.1.0",
  "description": "React specific components/utils for slate-yjs.",
  "keywords": [
    "slate",
    "yjs",
    "collaborative",
    "react"
  ],
  "type": "module",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.js"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "unpkg": "dist/index.global.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn run tsup && tsc --build --force -emitDeclarationOnly",
    "dev": "yarn run tsup --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BitPhinix/slate-yjs.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@slate-yjs/core": "^1.0.0",
    "@types/react": "^17.0.34",
    "@types/use-sync-external-store": "^0.0.3",
    "react": "^17.0.2",
    "slate": "^0.118.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "yjs": "^13.6.27"
  },
  "peerDependencies": {
    "@slate-yjs/core": "^1.0.0",
    "react": ">=16.8.0",
    "slate": ">=0.70.0",
    "slate-react": ">=0.70.0",
    "yjs": ">=13.5.29"
  },
  "bugs": {
    "url": "https://github.com/BitPhinix/slate-yjs/issues"
  },
  "homepage": "https://github.com/BitPhinix/slate-yjs#readme",
  "dependencies": {
    "use-sync-external-store": "^1.2.0",
    "y-protocols": "^1.0.5"
  }
}
